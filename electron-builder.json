{
  "productName": "AigisPlayer",
  "directories": {
    "app": "dist",
    "output": "app-builds"
  },
  "publish": [{
    "provider": "generic",
    "url": "http://player.aigis.me/assets/aigisplayer"
  }],
  "win": {
    "icon": "dist/app/favicon",
    "target": [{
      "target": "nsis",
      "arch": [
        "x64",
        "ia32"
      ]
    }]
  },
  "mac": {
    "icon": "dist/app/favicon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/app/favicon",
    "target": [
      "AppImage"
    ]
  }
}
