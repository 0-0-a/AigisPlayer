{
  "productName": "AigisPlayer",
  "directories": {
    "app": "dist",
    "output": "app-builds"
  },
  "publish": [
    {
      "provider": "generic",
      "url": "http://player.aigis.me/assets/aigisplayer"
    }
  ],
  "win": {
    "icon": "dist/favicon",
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }
    ]
  },
  "mac": {
    "icon": "dist/favicon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}